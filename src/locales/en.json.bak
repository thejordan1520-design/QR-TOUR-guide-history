      "fortaleza-san-felipe": {
        "name": "Fortaleza San Felipe",
        "description": "Historic 16th-century fortress",
        "history": "Fortaleza San Felipe was built in the 16th century by the Spanish to protect Puerto Plata's port from pirate attacks. Its thick coral stone walls and strategically placed cannons made it an impenetrable defense for over three centuries."
      },
      "teleferico-puerto-plata": {
        "name": "Puerto Plata Cable Car",
        "description": "The only cable car in the Caribbean",
        "history": "The Puerto Plata Cable Car is the only funicular in the Caribbean and offers breathtaking panoramic views of the city and the Atlantic Ocean. Ride up to Pico Isabel de Torres, where you'll find botanical gardens and a statue of Christ the Redeemer."
      },
      "calle-sombrillas": {
        "name": "Umbrella Street",
        "description": "Colorful artistic pedestrian street",
        "history": "Umbrella Street is a vibrant art installation that transforms an ordinary street into a tunnel of colors. Hundreds of multicolored umbrellas hang above, creating a unique visual spectacle symbolizing the joy and creativity of the Dominican people."
      },
      "calle-rosada": {
        "name": "Pink Street (Patio Doña Blanca)",
        "description": "Historic pink colonial street",
        "history": "Patio Doña Blanca, known as Pink Street, is a colonial architectural gem dating back to the 16th century. Its pastel pink walls and perfectly preserved Spanish architecture make it one of the most photographed streets in Puerto Plata."
      },
      "letrero-puerto-plata": {
        "name": "Puerto Plata Sign",
        "description": "Iconic sign on the boardwalk",
        "history": "The iconic Puerto Plata sign on the boardwalk has become the city's most recognizable symbol. Installed as part of the boardwalk's renovation, it represents local pride and is the favorite meeting point for tourists and locals to capture the perfect memory."
      },
      "museo-ambar": {
        "name": "Amber Museum",
        "description": "Prehistoric treasures in amber",
        "history": "The Amber Museum houses the world's most important collection of Dominican amber. These fossilized resins, 15-40 million years old, contain insects, plants, and small animals perfectly preserved, offering a unique window into the island's prehistoric past."
      },
      "ronfactory": {
        "name": "Brugal Rum Factory",
        "description": "Immerse yourself in the history and flavor of one of the Caribbean's most iconic distilleries.",
